<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/dashboardFragment">

    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.example.elucidate.DashboardFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_dashboardFragment_to_moodRatingFragment"
            app:destination="@id/moodRatingFragment" />
    </fragment>
    <fragment
        android:id="@+id/moodEntry"
        android:name="com.example.elucidate.MoodEntry"
        android:label="fragment_mood_entry"
        tools:layout="@layout/fragment_mood_entry"/>
    <fragment
        android:id="@+id/moodRatingFragment"
        android:name="com.example.elucidate.MoodRatingFragment"
        android:label="fragment_mood_rating"
        tools:layout="@layout/fragment_mood_rating" >
        <action
            android:id="@+id/action_moodRatingFragment_to_moodEntry"
            app:destination="@id/moodEntry" />
    </fragment>
</navigation>